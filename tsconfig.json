{
    "compilerOptions": {
        "target": "ES2018",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "outDir": "bin",
        "strict": true,
        "lib": [
            "ES2018",
            "ESNext"
        ],
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "experimentalDecorators": true
    },
    "files": [
        "infrastructure.ts"
    ]
}

